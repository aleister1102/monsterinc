{
  "input_config": {
    "input_urls": []
  },
  "httpx_runner_config": {
    "method": "GET",
    "request_uris": [],
    "threads": 20,
    "rate_limit": 0,
    "timeout_secs": 30,
    "retries": 2,
    "proxy": "",
    "follow_redirects": true,
    "max_redirects": 5,
    "custom_headers": {
      "X-Custom-Header": "MonsterIncValue"
    },
    "verbose": true,
    "tech_detect": true,
    "extract_title": true,
    "extract_status_code": true,
    "extract_location": true,
    "extract_content_length": true,
    "extract_server_header": true,
    "extract_content_type": true,
    "extract_ips": true,
    "extract_body": false,
    "extract_headers": true
  },
  "crawler_config": {
    "seed_urls": [],
    "user_agent": "MonsterIncCrawler/1.1",
    "request_timeout_secs": 20,
    "max_concurrent_requests": 50,
    "max_depth": 3,
    "respect_robots_txt": false,
    "include_subdomains": true,
    "scope": {
      "allowed_hostnames": ["insomnia1102.online", "example.com"],
      "disallowed_path_regexes": ["\\.js($|\\?)", "\\.txt($|\\?)"]
    },
    "max_content_length_bytes": 2
  },
  "reporter_config": {
    "output_dir": "reports",
    "items_per_page": 100,
    "embed_assets": true
  },
  "storage_config": {
    "parquet_base_path": "database",
    "compression_codec": "zstd"
  },
  "notification_config": {
    "discord_webhook_url": "YOUR_DISCORD_WEBHOOK_URL_HERE",
    "mention_role_ids": ["YOUR_ROLE_ID_1", "YOUR_ROLE_ID_2"],
    "notify_on_success": true,
    "notify_on_failure": true,
    "notify_on_scan_start": true,
    "notify_on_critical_error": true
  },
  "log_config": {
    "log_level": "info",
    "log_format": "json",
    "log_file": "logs.txt",
    "max_log_size_mb": 10,
    "max_log_backups": 100,
    "compress_old_logs": true
  }
}